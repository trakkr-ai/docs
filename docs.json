{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Trakkr Documentation",
  "colors": {
    "primary": "#0E9373",
    "light": "#10B981",
    "dark": "#047857"
  },
  "favicon": "/favicon.png",
  "logo": {
    "light": "/favicon.png",
    "dark": "/favicon.png"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Intro",
            "pages": [
              "introduction",
              {
                "group": "How Trakkr Works",
                "expanded": false,
                "pages": [
                  "quickstart",
                  "understanding-scores",
                  "reading-reports"
                ]
              },
              "plans",
              "troubleshooting"
            ]
          },
          {
            "group": "Features",
            "pages": [
              {
                "group": "Prompts",
                "expanded": false,
                "pages": [
                  "prompts",
                  "prompt-research"
                ]
              },
              "competitors",
              "citations",
              "outreach",
              "technical-audit",
              "optimize-content",
              {
                "group": "Prism",
                "expanded": false,
                "pages": [
                  "prism/introduction",
                  "prism/installation",
                  "prism/usage",
                  "prism/technical-details",
                  "prism/troubleshooting-and-faqs"
                ]
              },
              "sentiment-analysis",
              {
                "group": "Tracking",
                "expanded": false,
                "pages": [
                  "google-analytics",
                  "crawler-analytics",
                  "traffic-analytics"
                ]
              }
            ]
          },
          {
            "group": "My Account",
            "pages": [
              "enterprise",
              {
                "group": "Integrations",
                "expanded": false,
                "pages": [
                  "api",
                  "csv-exports",
                  "google-sheets",
                  "looker-studio"
                ]
              },
              "teams",
              "agency-features"
            ]
          }
        ]
      },
      {
        "tab": "API",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "api-reference/introduction",
              "api-reference/authentication",
              "api-reference/errors"
            ]
          },
          {
            "group": "API Endpoints",
            "expanded": true,
            "openapi": {
              "source": "openapi.yaml"
            }
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Open Trakkr",
          "href": "https://trakkr.ai/dashboard",
          "icon": "chart-line"
        },
        {
          "anchor": "Support",
          "href": "mailto:mack@trakkr.ai",
          "icon": "headset"
        },
        {
          "anchor": "Feature Requests",
          "href": "https://trakkr.featurebase.app/roadmap",
          "icon": "lightbulb"
        }
      ]
    }
  },
  "openapi": {
    "spec": "openapi.yaml"
  },
  "navbar": {
    "links": [
      {
        "label": "Contact Support",
        "href": "mailto:mack@trakkr.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Go to Dashboard",
      "href": "https://trakkr.ai/dashboard"
    }
  },
  "footer": {
    "socials": {
      "twitter": "https://twitter.com/trakkr_ai",
      "github": "https://github.com/trakkr-ai"
    }
  },
  "search": {
    "prompt": "Search Trakkr's docs..."
  },
  "feedback": {
    "thumbsRating": true,
    "suggestEdit": true,
    "raiseIssue": true
  },
  "api": {
    "playground": {
      "display": "interactive",
      "proxy": true
    },
    "examples": {
      "languages": ["curl", "javascript", "python"],
      "defaults": "required"
    }
  }
}
